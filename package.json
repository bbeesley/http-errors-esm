{
  "name": "http-errors-esm",
  "description": "Create HTTP error objects",
  "version": "1.0.0",
  "author": "Bill Beesley <bill@beesley.dev>",
  "contributors": [
    "Alan Plum <me@pluma.io>",
    "Douglas Christopher Wilson <doug@somethingdoug.com>",
    "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbeesley/http-errors-esm.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js"
    }
  },
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "depd": "2.0.0",
    "http-errors": "^2.0.0",
    "inherits": "2.0.4",
    "setprototypeof": "1.2.0",
    "statuses": "2.0.1",
    "toidentifier": "1.0.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/aws-lambda": "^8.10.101",
    "@types/http-errors": "^1.8.2",
    "@types/inherits": "^0.0.30",
    "@types/node": "^18.6.4",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "ava": "^4.3.1",
    "aws-sdk-client-mock": "^2.0.0-beta.1",
    "conventional-changelog-angular": "^5.0.13",
    "conventional-commit-types": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "documentation": "^14.0.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 16.14.0"
  },
  "scripts": {
    "clean": "rm -rf dist | true",
    "precompile": "npm run clean",
    "compile:esm": "tsc -p tsconfig-esm.json",
    "compile:cjs": "tsc -p tsconfig-cjs.json",
    "compile": "run-p compile:*",
    "dependabump": "ncu -u && rm -rf package-lock.json node_modules | true && npm i",
    "documentation": "./node_modules/.bin/documentation readme src/main/**.ts --github  --require-extension=ts --parse-extension=ts --section=api --shallow",
    "lint": "eslint --ext '.cjs' --ext '.mjs' --ext '.js' .",
    "pretest": "npm run clean && run-p lint compile",
    "test": "ava",
    "prerelease": "npm run documentation && npm run compile",
    "release": "semantic-release --debug"
  },
  "keywords": [
    "http",
    "error"
  ],
  "release": {
    "branches": [
      "master",
      "beta",
      "alpha"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "scope": "deps*",
              "release": "patch",
              "subject": "[security]*"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "README.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
